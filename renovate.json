{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "enabledManagers": [
    "gomod",
    "github-actions",
    "dockerfile"
  ],
  "dependencyDashboard": true,
  "labels": [
    "dependencies"
  ],
  "gomod": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchManagers": ["gomod"],
      "matchDepTypes": ["golang"],
      "enabled": true
    }
  ]
}
